{
  "benchmarks": [
    {
      "title": "As Of Join 100m work queue elements against 1m audit elements.",
      "generator_files": [ "workqueue-no-nulls-100m.json", "auditqueue-no-nulls-1m.json" ],
      "benchmark": {
          "statements" : [
              {
                  "title" : "preamble",
                  "text" : [
                      "import time",
                      "tag = 'no-nulls-100m-1m'",
                      "bench_name = 'aj-bench-' + tag",
                  ],
                  "timed" : 0
              },

              {
                  "title" : "load tables",
                  "text" : [
                      "from deephaven.ParquetTools import readTable", 
                      "workqueue = readTable('/data/workqueue-no-nulls-100m.parquet')",
                      "auditqueue = readTable('/data/auditqueue-no-nulls-1m.parquet')",
                  ],
                  "timed" : 1
              },

              {
                  "title" : "setup metrics",
                  "file" : "before.py",
                  "timed" : 0
              },

              {
                  "title" : "perform aj",
                  "text" : [
                      "time_start_ns = time.perf_counter_ns()",
                      "result = workqueue.aj(auditqueue, 'user_id, timestamp', 'audit_timestamp = timestamp')",
                      "time_end_ns = time.perf_counter_ns()",
                  ],
                  "timed" : 1
              },

              {
                  "title" : "post processing",
                  "file" : "after.py",
                  "timed" : 0
              }
          ]
      }
    }
  ]
}
