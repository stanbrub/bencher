{
  "benchmarks": [
    {
      "title": "Do a countBy on 30m rows",
      "generator_files": [ "relation-no-nulls-30m.json" ],
      "benchmark": {
          "statements" : [
              {
                  "title" : "preamble",
                  "text" : [
                      "import time",
                      "import deephaven.parquet as pt",
                      "tag = 'no-nulls-30m'",
                      "bench_name = 'by-bench-' + tag",
                  ],
                  "timed" : 0
              },

              {
                  "title" : "load tables",
                  "text" : "relation = pt.read('/data/relation-' + str(tag) + '.parquet')",
                  "timed" : 1
              },

              {
                  "title" : "setup metrics",
                  "file" : "before.py",
                  "timed" : 0
              },

              {
                  "title" : "perform countBy",
                  "text" : [
                      "time_start_ns = time.perf_counter_ns()",
                      "result = relation.view(formulas=['animal_id', 'adjective_id']).count_by('count', by=['animal_id'])",
                      "time_end_ns = time.perf_counter_ns()",
                  ],
                  "timed" : 1
              },

              {
                  "title" : "post processing",
                  "file" : "after.py",
                  "timed" : 0
              }
          ]
      }
    }
  ]
}
