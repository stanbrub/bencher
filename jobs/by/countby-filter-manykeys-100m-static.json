{
  "benchmarks": [
    {
      "title": "Do a countBy on 100m rows, 50M cardinality",
      "generator_files": [ "relation-manykeys-100m.json" ],
      "benchmark": {
        "statements" : [
          {
            "title" : "preamble",
            "text" : [
              "import time",
              "import deephaven.parquet as pt",
              "tag = 'no-nulls-100m'",
              "bench_name = 'countby-filter-manykeys-static'"
            ],
            "timed" : 0
          },

          {
            "title" : "load tables",
            "text" : "relation = pt.read('/data/relation-manykeys-100m.parquet').select('key1', 'Values')",
            "timed" : 1
          },

	  {
	    "title" : "setup agg list",
	    "file" : "by/agglist.py",
	    "timed" : 0
	  },

          {
            "title" : "setup metrics",
            "file" : "before.py",
            "timed" : 0
          },

          {
            "title" : "perform countBy",
            "text" : [
              "time_start_ns = time.perf_counter_ns()",
              "result = relation.aggBy(agg_list, 'key1').where('Count % 2 == 1').dropColumns('Count', 'key1').sumBy()",
              "time_end_ns = time.perf_counter_ns()",
              "processed_rows = relation.size()"
            ],
            "timed" : 1
          },

          {
            "title" : "post processing",
            "file" : "after.py",
            "timed" : 0
          }
        ]
      }
    }
  ]
}
