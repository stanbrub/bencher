{
  "benchmarks": [
    {
      "title": "Do a countBy plant column on 100m rows",
      "generator_files": [ "relation-no-nulls-100m.json" ],
      "benchmark": {
        "statements" : [
          {
            "title" : "preamble",
            "text" : [
              "import time",
              "import deephaven.parquet as pt",
              "tag = 'no-nulls-100m'",
              "bench_name = 'countby-plant-static'"
            ],
            "timed" : 0
          },

          {
            "title" : "load tables",
            "text" : "relation = pt.read('/data/relation-' + str(tag) + '.parquet').select(formulas=['plant_id'])",
            "timed" : 1
          },

          {
            "title" : "setup metrics",
            "file" : "before.py",
            "timed" : 0
          },

          {
            "title" : "perform countBy",
            "text" : [
              "time_start_ns = time.perf_counter_ns()",
              "result = relation.count_by('count', by=['plant_id'])",
              "time_end_ns = time.perf_counter_ns()",
              "processed_rows = relation.size"
            ],
            "timed" : 1
          },

          {
            "title" : "post processing",
            "file" : "after.py",
            "timed" : 0
          }
        ]
      }
    }
  ]
