{
  "benchmarks": [
    {
      "title": "As Of Join 100m work queue elements against 10k audit elements, both tables select()ed in memory first",
      "generator_files": [ "workqueue-no-nulls-100m.json", "auditqueue-no-nulls-10k.json" ],
      "benchmark": {
          "statements" : [
              {
                  "title" : "preamble",
                  "text" : [
                      "import time",
					  "import deephaven.parquet as pt",
                      "tag = 'no-nulls-100m-10k'",
                      "bench_name = 'select-aj-bench-' + tag",
                  ],
                  "timed" : 0
              },

              {
                  "title" : "load tables",
                  "text" : [
                      "workqueue = pt.read('/data/workqueue-no-nulls-100m.parquet').select()",
                      "auditqueue = pt.read('/data/auditqueue-no-nulls-10k.parquet').select()",
                  ],
                  "timed" : 1
              },

              {
                  "title" : "setup metrics",
                  "file" : "before.py",
                  "timed" : 0
              },

              {
                  "title" : "perform aj",
                  "text" : [
                      "time_start_ns = time.perf_counter_ns()",
                      "result = auditqueue.aj(workqueue, on=['user_id', 'timestamp'], joins=['work_timestamp = timestamp'])",
                      "time_end_ns = time.perf_counter_ns()",
                  ],
                  "timed" : 1
              },

              {
                  "title" : "post processing",
                  "file" : "after.py",
                  "timed" : 0
              }
          ]
      }
    }
  ]
}
