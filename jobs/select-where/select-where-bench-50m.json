{
  "benchmarks": [
    {
      "title": "Where filter over 50m relation table already in memory via select",
      "generator_files": [ "relation-50m.json" ],
      "benchmark": {
          "statements" : [
              {
                  "title" : "preamble",
                  "text" : [
                      "import time",
                      "import deephaven.ParquetTools as pt",
                      "tag = '100m'",
                      "bench_name = 'select-where-bench-' + tag",
                  ],
                  "timed" : 0
              },

              {
                  "title" : "load tables",
                  "text" : "relation = pt.readTable('/data/relation-' + str(tag) + '.parquet').select()",
                  "timed" : 1
              },

              {
                  "title" : "setup metrics",
                  "file" : "before.py",
                  "timed" : 0
              },

              {
                  "title" : "perform where over earlier computed select",
                  "text" : [
                      "time_start_ns = time.perf_counter_ns()",
                      "result = relation.where('!isNull(adjective_id) && adjective_id == animal_id')",
                      "time_end_ns = time.perf_counter_ns()"
                  ],
                  "timed" : 1
              },

              {
                  "title" : "post processing",
                  "file" : "after.py",
                  "timed" : 0
              }
          ]
      }
    }
  ]
}
