{
  "benchmarks": [
    {
      "title": "Sort 100m relation table on 2 columns",
      "generator_files": [ "relation-no-nulls-100m.json" ],
      "benchmark": {
          "statements" : [
              {
                  "title" : "preamble",
                  "text" : [
                      "import time",
                      "import deephaven.ParquetTools as pt",
                      "tag = 'no-nulls-100m'",
                      "bench_name = 'sort-bench-' + tag",
                  ],
                  "timed" : 0
              },

              {
                  "title" : "load tables",
                  "text" : "relation = pt.readTable('/data/relation-' + str(tag) + '.parquet')",
                  "timed" : 1
              },

              {
                  "title" : "setup metrics",
                  "file" : "before.py",
                  "timed" : 0
              },

              {
                  "title" : "perform sort",
                  "text" : [
                      "time_start_ns = time.perf_counter_ns()",
                      "result = relation.sort('animal_id', 'adjective_id')"
                      "time_end_ns = time.perf_counter_ns()",
                  ],
                  "timed" : 1
              },

              {
                  "title" : "post processing",
                  "file" : "after.py",
                  "timed" : 0
              }
          ]
      }
    }
  ]
}
