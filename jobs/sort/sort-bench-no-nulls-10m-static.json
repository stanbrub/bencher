{
  "benchmarks": [
    {
      "title": "Sort 10m relation table on 1 column",
      "generator_files": [ "relation-no-nulls-10m.json" ],
      "benchmark": {
          "statements" : [
              {
                  "title" : "preamble",
                  "text" : [
                      "import time",
                      "import deephaven.parquet as pt",
                      "tag = 'no-nulls-10m-static'",
                      "bench_name = 'sort-bench-' + tag"
                  ],
                  "timed" : 0
              },

              {
                  "title" : "load tables",
                  "text" : [
                      "relation = pt.read('/data/relation-no-nulls-10m.parquet')",
                  ],
                  "timed" : 1
              },

              {
                  "title" : "setup metrics",
                  "file" : "before.py",
                  "timed" : 0
              },

              {
                  "title" : "do sort",
                  "text" : [
                      "time_start_ns = time.perf_counter_ns()",
                      "result = relation.sort('animal_id')",
                      "time_end_ns = time.perf_counter_ns()",
                      "processed_rows = result.size"
                  ],
                  "timed" : 1
              },

              {
                  "title" : "post processing",
                  "file" : "after.py",
                  "timed" : 0
              }
          ]
      }
    }
  ]
}
