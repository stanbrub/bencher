{
  "benchmarks": [
    {
      "title": "Where filter over 100m relation table",
      "generator_files": [ "relation-100m.json" ],
      "benchmark": {
          "statements" : [
              {
                  "title" : "preamble",
                  "text" : [
                      "import time",
                      "import deephaven.ParquetTools as pt",
                      "tag = '100m'",
                      "bench_name = 'where-bench-' + tag",
                  ],
                  "timed" : 0
              },

              {
                  "title" : "load tables",
                  "text" : "relation = pt.readTable('/data/relation-' + str(tag) + '.parquet')",
                  "timed" : 1
              },

              {
                  "title" : "setup metrics",
                  "file" : "before.py",
                  "timed" : 0
              },

              {
                  "title" : "perform where",
                  "text" : [
                      "time_start_ns = time.perf_counter_ns()",
                      "result = relation.where('!isNull(adjective_id) && adjective_id == animal_id')",
                      "time_end_ns = time.perf_counter_ns()"
                  ],
                  "timed" : 1
              },

              {
                  "title" : "post processing",
                  "file" : "after.py",
                  "timed" : 0
              }
          ]
      }
    }
  ]
}
